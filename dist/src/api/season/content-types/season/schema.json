{
    "kind": "collectionType",
    "collectionName": "seasons",
    "info": {
        "singularName": "season",
        "pluralName": "seasons",
        "displayName": "Season"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "show": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::show.show",
            "inversedBy": "seasons"
        },
        "seasonNumber": {
            "type": "integer",
            "required": true
        },
        "title": {
            "type": "string"
        },
        "thumbnail": {
            "type": "media",
            "multiple": false,
            "allowedTypes": [
                "images",
                "files",
                "videos",
                "audios"
            ]
        },
        "episodes": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::episode.episode",
            "mappedBy": "season"
        }
    }
}
