{
    "kind": "collectionType",
    "collectionName": "shows",
    "info": {
        "singularName": "show",
        "pluralName": "shows",
        "displayName": "Show"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "title": {
            "type": "string",
            "required": true
        },
        "Description": {
            "type": "text"
        },
        "thumbnail": {
            "type": "media",
            "multiple": false,
            "allowedTypes": [
                "images",
                "files",
                "videos",
                "audios"
            ]
        },
        "showUrl": {
            "type": "string",
            "required": true
        },
        "seasons": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::season.season",
            "mappedBy": "show"
        }
    }
}
